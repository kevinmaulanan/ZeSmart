<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView"
            android:layout_width="168dp"
            android:layout_height="61dp"
            android:layout_marginStart="85dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="149dp"
            android:layout_marginBottom="630dp"
            android:text="Hi, Rotamitanian"
            android:textColor="@color/black"
            android:textSize="20sp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="83dp"
        android:layout_height="75dp"
        app:srcCompat="@drawable/fotoprofil" />


    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="100dp"
        android:layout_height="66dp"
        android:layout_marginStart="303dp"
        android:layout_marginEnd="-15dp"
        android:layout_marginBottom="340dp"
        android:backgroundTint="@color/white"
        app:srcCompat="@drawable/notif"
        tools:ignore="SpeakableTextPresentCheck" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"></LinearLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="384dp"
        android:layout_height="231dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="300dp"
        app:srcCompat="@drawable/gambar" />

    <SearchView
        android:id="@+id/search_action"
        android:layout_width="307dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="52dp"
        android:background="@drawable/bg"
        android:icon="@drawable/search"
        android:title="Search"
        app:showAsAction="always" >

    </SearchView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    </LinearLayout>

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="66dp"
        android:layout_height="88dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="310dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="1000dp"
        android:layout_weight="1"
        android:backgroundTint="@color/white"
        app:srcCompat="@drawable/group_347"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="66dp"
        android:layout_height="88dp"
        android:layout_marginStart="135dp"
        android:layout_marginTop="310dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="1000dp"
        android:layout_weight="1"
        android:backgroundTint="@color/white"
        tools:ignore="SpeakableTextPresentCheck"
        app:srcCompat="@drawable/group_348" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="66dp"
        android:layout_height="88dp"
        android:layout_marginStart="215dp"
        android:layout_marginTop="310dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="1000dp"
        android:layout_weight="1"
        android:backgroundTint="@color/white"
        tools:ignore="SpeakableTextPresentCheck"
        app:srcCompat="@drawable/group_349" />


    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="66dp"
        android:layout_height="88dp"
        android:layout_marginStart="295dp"
        android:layout_marginTop="310dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="100dp"
        android:layout_weight="1"
        android:backgroundTint="@color/white"
        app:srcCompat="@drawable/group_350"
        tools:ignore="SpeakableTextPresentCheck" />


</RelativeLayout>