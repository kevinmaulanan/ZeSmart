<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ScheduleActivity">


    <View
        android:id="@+id/view"
        android:layout_width="355dp"
        android:layout_height="72dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="475dp"
        android:background="@drawable/rounded1"
        android:backgroundTint="@color/colorSecondary"
        tools:ignore="SpeakableTextPresentCheck" />

    <View
        android:id="@+id/view4"
        android:layout_width="235dp"
        android:layout_height="41dp"
        android:layout_marginTop="430dp"
        android:background="@drawable/rounded3"
        android:backgroundTint="#102755"

        tools:ignore="DuplicateSpeakableTextCheck" />

    <View
        android:id="@+id/button"
        android:layout_width="420dp"
        android:layout_height="126dp"
        android:layout_marginTop="4dp"
        android:background="#102755"
        android:text="November 2021"
        android:textAllCaps="false"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <org.naishadhparmar.zcustomcalendar.CustomCalendar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/customcalendar"
        android:layout_marginTop="120dp"
        />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="139dp"
        android:layout_height="37dp"
        android:layout_marginStart="48dp"
        android:text="Schedule"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="italic"
        android:layout_marginTop="430dp" />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@id/bottomNavigationView"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="262dp"
        android:layout_height="52dp"
        android:text="November 2021"
        android:textColor="#FFFFFF"
        android:textSize="32sp"
        android:textStyle="bold"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="40dp"/>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="550dp"
        android:backgroundTint="@android:color/transparent"
        android:background="#102755"
        app:itemIconTint="#FFFFFF"
        app:menu="@menu/bottom_navigation"

        />


</androidx.coordinatorlayout.widget.CoordinatorLayout>